  <Variable
    Name="project"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="contract"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select bc.id, bc.title, bc.username, bc.contact_email, bc.contact_role, bc.fullname ||' ('||bc.contact_role||')' as display
from phe_business_contact bc
where bc.id in
(select distinct pbc.bcid
from phe_bc_cn pbc
where pbc.contract_number in(
select contract_number
from phe_project pp, phe_project_cn ppc
where pp.id = ppc.project_id and ppc.project_id=:project and contract_number = :contract)) and bc.is_delete=0]]>
  </SQLQuery>
