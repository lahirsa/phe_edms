  <SQLQuery>
    <![CDATA[select * from 
(
SELECT rownum as unique_id, ppar.delegate_to as receiver,pdd.id,pdd.transmittal_id, pdd.content_id, pdd.type,pdd.format,pdd.uploader,pdd.upload_date,pdd.doc_name,
pdd.doc_title,pdd.doc_number,pdd.doc_source,pdd.doc_status,pdd.pages,pdd.remarks,pdd.revision, pdd.publish_status,
pdd.doc_type,pdd.seq_id ,pov.value as display_status
from phe_delegate ppar, phe_dswf_doc pdd, phe_option_values pov
where pdd.transmittal_id = :transmittal_id and pdd.doc_type = 'Supporting' and pdd.transmittal_id = ppar.transmittal_id
and (pov.description <> '0' and UPPER(pov.CODE) = UPPER('DOCSTATUS'))  and pov.description = pdd.doc_status
and ppar.delegate_to = :username
order by receiver
) ab left join (
select pdp.did,pdp.action, pdp.recipient ,pdc.description,pdp.notes from phe_doc_process pdp, phe_dswf_config pdc where pdp.transmittal_id = :transmittal_id and pdc.grouping = 'Doc Review' and pdp.action = pdc.code
)ac on ab.id = ac.did and ac.recipient = ab.receiver]]>
  </SQLQuery>
