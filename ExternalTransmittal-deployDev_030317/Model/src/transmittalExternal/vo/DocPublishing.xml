<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DocPublishing"
  Version="11.1.1.61.92"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="transmittalExternal.vo.DocPublishing_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="transmittalId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select distinct pd.id,pd.publish_status
from phe_doc_process pp,phe_dswf_doc pd
where pd.transmittal_id = :transmittalId
and pp.transmittal_id = pd.transmittal_id 
and pp.did = pd.id 
and pd.doc_status in ('IFP')
and pd.doc_type = 'Supporting']]>
  </SQLQuery>
  <ViewAttribute
    Name="Id"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="ID"
    Expression="ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PublishStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PUBLISH_STATUS"
    Expression="PUBLISH_STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="transmittalExternal.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
