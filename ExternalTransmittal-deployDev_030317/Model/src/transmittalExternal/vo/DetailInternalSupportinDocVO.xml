  <Variable
    Name="transmittal_id"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.object.viewObject.transmittal_id]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[select * from(select ab.*,nvl(ac.did,ab.id)as did,nvl(ac.action,'0')as action,nvl(ac.recipient,ab.receiver)as recipient,ac.description,ac.notes from ( SELECT ROWNUM AS unique_id,
         ppar.receiver,
         pdd.id,
         pdd.transmittal_id,
         pdd.content_id,
         pdd.TYPE,
         pdd.format,
         pdd.uploader,
         pdd.upload_date,
         pdd.doc_name,
         pdd.doc_title,
         pdd.doc_number,
         pdd.doc_source,
         pdd.doc_status,
         pdd.pages,
         pdd.remarks,
         pdd.revision,
         pdd.publish_status,
         pdd.doc_type,
         pdd.seq_id,
         pov.VALUE AS display_status
    FROM phe_dswf_participant ppar, phe_dswf_doc pdd, phe_option_values pov
   WHERE     pdd.transmittal_id = :transmittal_id
         AND pdd.doc_type = 'Supporting'
         AND pdd.transmittal_id = ppar.transmittal_id
         AND (pov.description <> '0' AND UPPER (pov.CODE) = UPPER ('DOCSTATUS'))
         AND pov.description = pdd.doc_status
            ORDER BY receiver)ab,
(SELECT pdp.did,
       nvl(pdp.action,'0')as action,
       pdp.recipient,
       pdc.description,
       pdp.notes
  FROM phe_dswf_participant ppar, phe_doc_process pdp, phe_dswf_config pdc
 WHERE     pdc.GROUPING = 'Doc Review'
       AND pdp.action = pdc.code
       AND ppar.receiver = pdp.recipient
       AND pdp.transmittal_id = ppar.transmittal_id
       AND ppar.enddate IS NOT NULL) ac where ab.id = ac.did(+) and ac.recipient(+) = ab.receiver 
union
select * from 
(select rownum as unique_id, pdp.delegate_to as receiver,pdd.id,pdd.transmittal_id, pdd.content_id, pdd.type,pdd.format,pdd.uploader,pdd.upload_date,pdd.doc_name,
pdd.doc_title,pdd.doc_number,pdd.doc_source,pdd.doc_status,pdd.pages,pdd.remarks,pdd.revision, pdd.publish_status,
pdd.doc_type,pdd.seq_id ,pov.value as display_status, pdd.id as did, 3 as action, pdp.delegate_to as recipient, 'Delegated' as description, null as notes
from phe_delegate pdp, phe_dswf_doc pdd, phe_option_values pov
where pdd.transmittal_id = :transmittal_id and pdd.doc_type = 'Supporting' and pdd.transmittal_id = pdp.transmittal_id
and (pov.description <> '0' and UPPER(pov.CODE) = UPPER('DOCSTATUS'))  and pov.description = pdd.doc_status
order by receiver))]]>
  </SQLQuery>
