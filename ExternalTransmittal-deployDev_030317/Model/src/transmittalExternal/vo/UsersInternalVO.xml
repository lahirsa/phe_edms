<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UsersInternalVO"
  Version="11.1.1.66.73"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  OrderBy="&quot;FULLNAME&quot;">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="transmittalExternal.vo.UsersInternalVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT ur.dname AS USERNAME, ur.dfullname AS FULLNAME, ur.demail AS EMAIL
  FROM users ur
 WHERE    (ur.dname NOT IN ('weblogic', 'OracleSystemUser', 'sysadmin')
    or  ur.dname NOT IN (SELECT pbc.username AS USERNAME FROM phe_business_contact pbc))
       AND ur.dfullname IS NOT NULL]]>
  </SQLQuery>
  <ViewAttribute
    Name="Username"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="USERNAME"
    Expression="USERNAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Fullname"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FULLNAME"
    Expression="FULLNAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Email"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="80"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EMAIL"
    Expression="EMAIL"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="80"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Username"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="transmittalExternal.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
